{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.ts", "src/**/*", "src/**/*.vue", "__tests__/e2e"],
  "exclude": [
    "src/**/__tests__/*",
    "tests/**/*",
    "src/tests/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "baseUrl": "",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
